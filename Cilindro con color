package MisProgramas;

public class CilindroConColor extends Cilindro{
	private String color;
	/**
	 * @param altura
	 * @param radio
	 * @param color
	 */
	public CilindroConColor(double altura, double radio, String color) {
		super(altura, radio);
		this.color = color;
	}
	/**
	 * @return the color
	 */
	public String getColor() {
		return color;
	}
	/**
	 * @param color the color to set
	 */
	public void setColor(String color) {
		this.color = color;
	}
}
