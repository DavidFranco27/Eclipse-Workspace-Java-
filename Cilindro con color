package MisProgramas;

public class CilindroConColor extends Cilindro{
	private String color;

	/**
	 * @param base
	 * @param altura
	 * @param radio
	 * @param color
	 */
	public CilindroConColor(double base, double altura, double radio, String color) {
		super(base, altura, radio);
		this.color = color;
	}

	/**
	 * @return the color
	 */
	public String getColor() {
		return color;
	}

	/**
	 * @param color the color to set
	 */
	public void setColor(String color) {
		this.color = color;
	}
}
