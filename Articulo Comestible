package MisProgramas;

public class ArticuloComestible extends Articulo{
	private String fechaCaducidad;
	private String tipo;
	/**
	 * @param articuloID
	 * @param nombre
	 * @param formato
	 * @param vendedor
	 * @param precio
	 * @param fechaCaducidad
	 * @param tipo
	 */
	//Inicializar
	public ArticuloComestible(int articuloID, String nombre, String formato, String vendedor, double precio,
			String fechaCaducidad, String tipo) {
		super(articuloID, nombre, formato, vendedor, precio);
		this.fechaCaducidad = fechaCaducidad;
		this.tipo=tipo;
	}
	/**
	 * @return the fechaCaducidad
	 */
	public String getFechaCaducidad() {
		return fechaCaducidad;
	}
	/**
	 * @param fechaCaducidad the fechaCaducidad to set
	 */
	public void setFechaCaducidad(String fechaCaducidad) {
		this.fechaCaducidad = fechaCaducidad;
	}
	/**
	 * @return the tipo
	 */
	public String getTipo() {
		return tipo;
	}
	/**
	 * @param tipo the tipo to set
	 */
	public void setTipo(String tipo) {
		this.tipo = tipo;
	}
}
